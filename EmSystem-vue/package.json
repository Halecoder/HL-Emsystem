{
  "name": "vue-admin-template",
  "version": "4.4.0",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "author": "Pan <panfree23@gmail.com>",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview-index": "node build/index.js --preview",
    "preview": "node src/views/lookresume/scripts/preview.js",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "export": "concurrently \"npm run dev\" \"npm run pdf\" --success first --kill-others --raw",
    "pdf": "node src/views/lookresume/scripts/export.js"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "^0.4.0",
    "axios": "0.18.1",
    "clipboard": "^2.0.11",
    "core-js": "3.6.5",
    "element-china-area-data": "^5.0.2",
    "element-ui": "2.13.2",
    "file-saver": "^2.0.5",
    "fms-form-generator": "^1.4.6",
    "font-awesome": "4.7.0",
    "form-gen-parser": "^1.0.3",
    "generic-pool": "^3.7.1",
    "jquery": "^3.6.3",
    "js-cookie": "2.2.0",
    "jwt-decode": "^3.1.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "material-design-icons": "3.0.1",
    "material-icons": "0.2.3",
    "normalize.css": "7.0.0",
    "npm-font-open-sans": "1.1.0",
    "npm-font-open-sans-condensed": "1.0.3",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "quill": "^1.3.7",
    "raleway-webfont": "3.0.1",
    "readline": "^1.3.0",
    "roboto-fontface": "0.10.0",
    "source-sans-pro": "2.20.2",
    "swiper": "^5.4.5",
    "v-scroll-lock": "^1.3.1",
    "vue": "2.6.10",
